{
  "name": "code-voyage",
  "version": "2.0.0",
  "private": true,
  "description": "A high-impact, fully JSON-driven Next.js template for documentation and marketing sites.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:images": "vitest run tests/lib/images.test.ts tests/lib/content.test.ts tests/integration/image-display.test.ts tests/integration/image-quality.test.ts tests/integration/build-validation.test.ts",
    "test:all": "npm run test && npm run test:images",
    "prebuild": "node -e \"if (process.env.SKIP_CHECK !== 'true') { require('child_process').execSync('npm run test:images', {stdio: 'inherit'}); }\"",
    "check": "npm run check:images && npm run check:links",
    "check:images": "node scripts/check-images.js",
    "check:links": "node scripts/check-links.js",
    "fetch-images": "node scripts/fetch-wikimedia-images.js",
    "fetch-images-multi": "node scripts/fetch-onsen-images-multi-api.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-slot": "^1.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.17.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.9"
  }
}
